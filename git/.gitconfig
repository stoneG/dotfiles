[user]
	name = sitong
	email = sitong@myeatclub.com
[core]
	editor = vim
[credential]
	helper = osxkeychain
[diff]
    tool = vimdiff
[merge]
    tool = vimdiff
[alias]
	last = log -1 HEAD
	last2 = log -2 HEAD
    undo = reset --hard HEAD~1
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
[http]
	postBuffer = 524288000
[color]
	ui = true
[push]
	default = simple
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
